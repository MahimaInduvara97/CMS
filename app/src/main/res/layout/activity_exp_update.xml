    <EditText
        android:id="@+id/editText7"
        android:layout_width="147dp"
        android:layout_height="36dp"
        android:layout_marginTop="284dp"
        android:layout_marginEnd="24dp"
        android:ems="10"
        android:inputType="textPersonName"
